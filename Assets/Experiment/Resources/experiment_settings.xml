<?xml version="1.0" encoding="Windows-1252"?>
<ExperimentSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="Energyscape">
  <DatabaseSettings>
    <Server>localhost</Server>
    <Schema>energyscape_eve</Schema>
    <User>energyscape</User>
    <Password>MainExperiment2018</Password>
  </DatabaseSettings>
  <SensorSettings Labchart="true" H7Server="false">
    <Sensors>
      <Sensor>QuestionnaireSystem</Sensor>
      <Sensor>Fixation</Sensor>
      <Sensor>LabChart</Sensor>
      <Sensor>Video</Sensor>
      <Sensor>Decision</Sensor>
      <Sensor>User_Choice</Sensor>
      <Sensor>Questionnaire</Sensor>
    </Sensors>
  </SensorSettings>
  <ParameterSettings>
    <Parameters>
      <Parameter>training_stimuli_order</Parameter>
      <Parameter>landscape_stimuli_order</Parameter>
      <Parameter>perception_1_order</Parameter>
      <Parameter>perception_2_order</Parameter>
      <Parameter>perception_3_order</Parameter>
      <Parameter>perception_4_order</Parameter>
      <Parameter>perception_5_order</Parameter>
      <Parameter>perception_6_order</Parameter>
      <Parameter>researcher_id</Parameter>
      <Parameter>Labchart File Name</Parameter>
    </Parameters>
  </ParameterSettings>
  <QuestionnaireSettings>
    <Questionnaires>
      <Questionnaire>energyscape_pre</Questionnaire>
    </Questionnaires>
  </QuestionnaireSettings>
  <SceneSettings>
    <Scenes>
      <Scene>LabchartStartScene</Scene>
      <Scene>energyscape_pre.xml</Scene>
    </Scenes>
  </SceneSettings>
  <LabchartSettings>
    <Path>PathToLabchartFiles\\</Path>
    <Commenters>
      <Sensor>QuestionnaireSystem</Sensor>
      <Sensor>LabChart</Sensor>
    </Commenters>
  </LabchartSettings>
  <UISettings>  
    <Resolution X="1920" Y="1080" />
  </UISettings>
</ExperimentSettings>