<?xml version="1.0" encoding="Windows-1252"?>
<QuestionSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="TestSet">
  <Questions>
    <InfoScreen Name="example_info" NColumns="1" NRows="1">
      <Text>People questionnaire:Perception of people in the neighborhood</Text>
    </InfoScreen>
    <TextQuestion Name="exercise" NColumns="1" NRows="1">
      <Text>What type of exercise have you done recently?</Text>
    </TextQuestion>
    <TextQuestion Name="born_in" NColumns="1" NRows="2">
      <Text>Where were you born?</Text>
      <RowLabels>
        <Label Text="Country:" />
        <Label Text="City:" />
      </RowLabels>
    </TextQuestion>
    <ChoiceQuestion Name="yes_no" NColumns="1" NRows="2" Choice="Single">
      <Text>Is this a Yes/No Question?</Text>
      <RowLabels>
        <Label Text="Yes" Output="1" />
        <Label Text="No" Output="0" />
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="multiple_yes_no" NColumns="2" NRows="5" Choice="Single">
      <Text>Answer quickly without thinking:</Text>
      <ColumnLabels>
        <Label Text="Yes" Output="1" />
        <Label Text="No" Output="0" />
      </ColumnLabels>
      <RowLabels>
        <Label Text="Do you like questionnaires?" />
        <Label Text="Have you ever been to New York?" />
        <Label Text="Coffee with Milk?" />
        <Label Text="Coffee with Sugar?" />
        <Label Text="Black Coffee?" />
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="coffe_consumption" NColumns="1" NRows="2" Choice="Single">
      <Text>Did you have coffee, espresso, or another beverage containing caffeine in the past 24 hours?</Text>
      <RowLabels>
        <Label Text="If yes, how many hours ago" Output="1" Answerable="true" />
        <Label Text="No, I did not have any caffeine" Output="0" />
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="band_judgement" NColumns="3" NRows="4" Choice="Multiple">
      <Text>Please check with which assessment you agree most about the bands:</Text>
      <ColumnLabels>
        <Label Text="Mainstream" Output="0" />
        <Label Text="Best of their Genre" Output="1" />
        <Label Text="Original" Output="2" />
      </ColumnLabels>
      <RowLabels>
        <Label Text="Coldplay" />
        <Label Text="Maroon5" />
        <Label Text="Nsync" />
        <Label Text="Red Hot Chili Peppers" />
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="likert_scale" NColumns="7" NRows="1" Choice="Single">
      <Text>Is this a good question?</Text>
      <ColumnLabels>
        <Label Text="Not at all" Output="0" />
        <Label Text="" Output="1" />
        <Label Text="" Output="2" />
        <Label Text="It is okay" Output="3" />
        <Label Text="" Output="4" />
        <Label Text="" Output="5" />
        <Label Text="Very good" Output="6" />
      </ColumnLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="multi_c_text" NColumns="1" NRows="8" Choice="Multiple">
      <Text>Which animals do you like:</Text>
      <RowLabels>
        <Label Text="Bears" Output="0" />
        <Label Text="Lions" Output="1" />
        <Label Text="Frogs" Output="2" />
        <Label Text="Axolotls" Output="2" />
        <Label Text="Humans" Output="2" />
        <Label Text="Bats" Output="2" />
        <Label Text="None" Output="2" />
        <Label Text="Other:" Output="2" Answerable="true" />
      </RowLabels>
    </ChoiceQuestion>
    <ChoiceQuestion Name="image_example" NColumns="1" NRows="3" Choice="Single">
      <Text>Which image looks the best?</Text>
      <RowLabels>
        <Label Text="This" Output="0" Image="Images/test_image" />
        <Label Text="This" Output="1" Image="Images/test_image" />
        <Label Text="This" Output="2" Image="Images/test_image" />
      </RowLabels>
    </ChoiceQuestion>
    <ScaleQuestion Name="amuesment_scale" NColumns="9" NRows="1" Scale="Line">
      <Text>How much did you feel AMUSEMENT?</Text>
      <LeftLabel>Did not experience at all</LeftLabel>
      <RightLabel>Strongest experience ever felt</RightLabel>
    </ScaleQuestion>
    <ScaleQuestion Name="SAM1_pleasure" NColumns="9" NRows="1" Scale="Pleasure">
      <Text>Please rate how happy-unhappy you actually felt</Text>
      <LeftLabel>SAD</LeftLabel>
      <RightLabel>CHEERFUL</RightLabel>
    </ScaleQuestion>
    <ScaleQuestion Name="SAM1_arousal" NColumns="9" NRows="1" Scale="Arousal">
      <Text>Please rate how excited - calm you actually felt</Text>
      <LeftLabel>QUIET</LeftLabel>
      <RightLabel>ACTIVE</RightLabel>
    </ScaleQuestion>
    <ScaleQuestion Name="SAM1_dominance" NColumns="9" NRows="1" Scale="Dominance">
      <Text>Please rate how controlled vs. in-control you actually felt</Text>
      <LeftLabel>DEPENDENT</LeftLabel>
      <RightLabel>INDEPENDENT</RightLabel>
    </ScaleQuestion>
    <LadderQuestion Name="swiss_status" NColumns="1" NRows="1">
      <Text>Swiss comparison of social status</Text>
      <LadderText>At the TOP of the ladder are the people who are the best off. The lower you are, the closer you are to the people at the very bottom. Where would you place yourself on this ladder, compared to all the other people in Switzerland?</LadderText>
    </LadderQuestion>
    <ChoiceQuestion Name="flu_med" NColumns="1" NRows="2" Choice="Single">
      <Text>Have you taken any medication for cold or flu symptoms today?</Text>
      <Jumps>
        <Jump Destination="inflam_med" Activator="FT" />
      </Jumps>
      <RowLabels>
        <Label Text="Yes" Output="1" />
        <Label Text="No" Output="0" />
      </RowLabels>
    </ChoiceQuestion>
    <TextQuestion Name="flu_med_name" NColumns="1" NRows="1">
      <Text>Which medication(s)?</Text>
    </TextQuestion>
    <ChoiceQuestion Name="inflam_med" NColumns="1" NRows="2" Choice="Single">
      <Text>Have you taken any anti-inflammatory medication today?</Text>
      <Jumps>
        <Jump Destination="*" Activator="FT" />
      </Jumps>
      <RowLabels>
        <Label Text="Yes" Output="1" />
        <Label Text="No" Output="0" />
      </RowLabels>
    </ChoiceQuestion>
    <TextQuestion Name="inflam_med_name" NColumns="1" NRows="1">
      <Text>Which medication(s)?</Text>
    </TextQuestion>
  </Questions>
</QuestionSet>